{
  "name": "tree-sitter-gomod",
  "version": "1.0.2",
  "description": "A tree-sitter grammar for go.mod files",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camdencheek/tree-sitter-gomod.git"
  },
  "keywords": [
    "tree-sitter",
    "go"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "author": "Camden Cheek <camden@ccheek.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/camdencheek/tree-sitter-gomod/issues"
  },
  "homepage": "https://github.com/camdencheek/tree-sitter-gomod#readme",
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.4"
  },
  "tree-sitter": [
    {
      "scope": "source.gomod",
      "file-types": [
        "go.mod"
      ]
    }
  ]
}
